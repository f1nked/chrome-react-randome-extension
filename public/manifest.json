{
   "name": "RandoMe",
   "description": "Random web surfing",
   "permissions": [
    "tabs",
    "storage"
 ],
"host_permissions":[
    "<all_urls>",
    "https://www.google.com/*",
    "http://www.google.com/*"
 ],
   "version": "1.0",
   "manifest_version": 3,
   "action": {
       "default_popup": "index.html",
       "default_title": "RandoMe"
   },
   "icons": {
    "16": "logo16.png",
    "32": "logo16.png",
    "48": "logo48.png",
    "128": "logo48.png"
  }
}